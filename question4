static String collegeName = "CIT"; 
{
collegeName = "MIT"; 
} 

Student(int id, String name){ 
stdId = id; 
stdName = name; 
} 

void display() 
{ 
System.out.println(stdId+"  "+stdName+"  "+collegeName); 
} 
} 

